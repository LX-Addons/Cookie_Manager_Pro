{
  "name": "cookie-manager-pro",
  "displayName": "Cookie Manager Pro",
  "version": "1.1.3",
  "description": "高级Cookie管理，支持白名单/黑名单功能和选择性Cookie清除",
  "type": "module",
  "packageManager": "npm@10.0.0",
  "scripts": {
    "dev": "wxt",
    "build": "wxt build",
    "zip": "wxt zip",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css}\"",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "changelog": "git-cliff --output CHANGELOG.md",
    "changelog:latest": "git-cliff --latest"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "wxt": "0.20.11"
  },
  "devDependencies": {
    "@playwright/test": "1.58.2",
    "@testing-library/dom": "10.0.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.19.0",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "8.55.0",
    "@typescript-eslint/parser": "8.55.0",
    "@vitest/coverage-istanbul": "4.0.18",
    "@vitest/ui": "4.0.18",
    "@wxt-dev/module-react": "1.1.2",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.5.0",
    "git-cliff": "2.12.0",
    "globals": "17.3.0",
    "happy-dom": "20.6.0",
    "jsdom": "28.0.0",
    "prettier": "3.8.1",
    "typescript": "5.3.3",
    "vitest": "4.0.18"
  },
  "overrides": {
    "msgpackr": "1.11.8",
    "content-security-policy-parser": "0.6.0",
    "svelte": "4.2.19",
    "esbuild": "0.25.0"
  }
}
